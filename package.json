  "bugs": {
    "url": "https://github.com/datagutt/pynab.js/issues"
  },
  "homepage": "https://github.com/datagutt/pynab.js#readme",
  "devDependencies": {
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@types/node": "^13.7.0",
    "@types/qs": "^6.9.0",
    "abab": "^2.0.3",
    "color-rgba": "^2.1.1",
    "isomorphic-fetch": "^2.2.1",
    "qs": "^6.9.1",
    "rimraf": "^3.0.1",
    "tslint": "^6.0.0"
  }
